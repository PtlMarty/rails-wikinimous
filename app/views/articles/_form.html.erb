<%= form_with model: article do |f|%>
  <div class="card ">
    <div class="card-body d-flex flex-column m-3">
      <%= f.label :title %>
      <%= f.text_field :title %>
      <%= f.label :content %>
      <%= f.text_area :content %>
      <%= f.submit class:"btn btn-primary mt-3"%>
    </div>
  </div>
<% end %>
